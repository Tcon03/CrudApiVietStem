<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="PrimaryColor" Color="#1b1448"/>
    <SolidColorBrush x:Key="ColorButtonPurpule" Color="#634BD4 "/>
    <SolidColorBrush x:Key="ColorBlue" Color="#4EB1B6"/>
    <SolidColorBrush x:Key="ColorRed" Color="#CC2B52"/>
    <SolidColorBrush x:Key="PrimaryBlueColor" Color="#5B7FFF"/>
    <SolidColorBrush x:Key="PrimaryGrayColor" Color="#BFBFC4"/>
    <SolidColorBrush x:Key="CloseColor" Color="#9ACBD0"/>

    <LinearGradientBrush x:Key="GenerateButtonGradient" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Color="#00F2FF" Offset="0.0" />
        <GradientStop Color="#4C28E1" Offset="1.0" />
    </LinearGradientBrush>
</ResourceDictionary>